# MongoDB Configuration
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/?retryWrites=true&w=majority&appName=YourApp

# Azure Web PubSub Configuration
WEB_PUBSUB_CONNECTION_STRING=Endpoint=https://your-pubsub.webpubsub.azure.com;AccessKey=YOUR_ACCESS_KEY_HERE;Version=1.0;
WEB_PUBSUB_HUB_NAME=Hub

# Server Configuration
PORT=3002
NODE_ENV=test

# Test Logging
LOG_LEVEL=error

# Test Timeouts
TEST_TIMEOUT=10000

# Load Test Configuration
LOAD_TEST_VUS=100
LOAD_TEST_DURATION=5m

# Performance Thresholds
LATENCY_P95_THRESHOLD=2000
LATENCY_P99_THRESHOLD=5000
SUCCESS_RATE_THRESHOLD=0.95
