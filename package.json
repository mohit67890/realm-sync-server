{
  "name": "sync-demo",
  "version": "1.0.0",
  "description": "Azure Web PubSub + Socket.IO + MongoDB Timestamp-based Sync",
  "main": "dist/server/index.js",
  "scripts": {
    "dev:server": "nodemon --exec ts-node server/index.ts",
    "dev:test-server": "ts-node server/test-socketio-server.ts",
    "dev:real-sync-server": "ts-node server/real-sync-test-server.ts",
    "dev:client": "ts-node client/example.ts",
    "dev:client2": "ts-node client/example.ts demo-user-2",
    "build": "tsc",
    "start": "node dist/server/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:unit": "jest --testPathPattern=database.test.ts",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "jest --testPathPattern=e2e --runInBand",
    "test:stress": "jest --testPathPattern=stress --runInBand --testTimeout=120000",
    "test:all": "./scripts/run-all-tests.sh",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"**/*.ts\""
  },
  "keywords": [
    "sync",
    "realtime",
    "azure",
    "socket.io",
    "mongodb"
  ],
  "author": "Mohit Garg",
  "license": "MIT",
  "dependencies": {
    "@azure/web-pubsub": "^1.1.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.3.0",
    "socket.io": "^4.6.2",
    "socket.io-client": "^4.6.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^10.1.4",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
